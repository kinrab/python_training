pipeline 

{ agent any

  stages 
 {

    stage('Clone Repository') 
    {

        steps 
        {

            git '<https://github.com/your-repo/your-project.git>'

        }

    }

    stage('Preparation') 
    {

        steps 
        {          
           bat 'python -m venv venv'  
           bat '.venv\Scripts\activate'
           pip install -r requirements.txt
          
        }

    }

    stage('Run tests') 
    {

        steps 
        {

            // Exammple

           bat 'pytest TEST\test.phones.py'
        }

    }

  }

}
